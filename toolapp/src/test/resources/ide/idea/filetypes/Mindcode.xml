<filetype binary="false" default_extension="mnd" description="Mindcode source file" name="Mindcode">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="0x" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="${mindcode-keywords}" ignore_case="false" />
    <keywords2 keywords="${mindustry-builtins}" ignore_case="false" />
    <keywords3 keywords="${mlog-keywords}" ignore_case="false" />
  </highlighting>
  <extensionMap>
    <mapping ext="mnd" />
  </extensionMap>
</filetype>