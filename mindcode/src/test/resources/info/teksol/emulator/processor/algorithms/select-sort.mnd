allocate stack in bank1[0...512];
param ARRAY = bank2;

for i in 0 ... SIZE - 1 do
    min = ARRAY[i];
    min_index = i;
    for j in i + 1 ... SIZE do
        curr = ARRAY[j];
        if curr < min then
            min = curr;
            min_index = j;
        end;
    end;
    ARRAY[min_index] = ARRAY[i];
    ARRAY[i] = min;
end;

