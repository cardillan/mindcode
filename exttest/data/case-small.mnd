def identifyBlock(block)
    case block
        when @message then "M";
        when @switch then "S";
        when @memory-bank, @memory-cell then "R";
        else "0";
    end;
end;

for i in 0 ... @blockCount do
    print(identifyBlock(lookup(:block, i)));
end;
